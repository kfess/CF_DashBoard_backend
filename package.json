{
  "name": "cf-dashboard-backend",
  "version": "0.0.0",
  "private": true,
  "description": "Codeforces Problems Dashboard Backend",
  "author":"kfess",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kfess/Codeforces_Problems_backend"
  },
  "bugs": {
    "url": "https://github.com/kfess/Codeforces_Problems_backend/issues"
  },
  "scripts": {
    "build": "tsc",
    "db-update": "tsc && ts-node dist/daily_update.js",
    "format": "npm run sort-package-json && npm run prettier ",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write . --ignore-path .gitignore",
    "sort-package-json": "sort-package-json",
    "start": "tsc && ts-node dist/index.js",
    "test": "jest --config jest.config.ts --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,html,json,md}": "prettier --write",
    "package.json": [
      "prettier --write",
      "sort-package-json"
    ],
    ".eslintrc.js": "prettier --write"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@vendia/serverless-express": "^4.10.1",
    "axios": "^1.3.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dayjs-plugin-utc": "^0.1.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "prisma": "^4.12.0",
    "sort-package-json": "^2.5.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/passport": "^1.0.12",
    "@types/passport-github": "^1.1.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
  },
  "_moduleAliases": {
    "@": "./src"
  }
}
